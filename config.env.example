# Configuration for run_extractor.sh
# Copy to config.env and adjust as needed

# GitHub repository to clone
GITHUB_REPO=https://github.com/komapc/ido-esperanto-extractor.git

# Directory name after cloning
EXTRACTOR_DIR=ido-esperanto-extractor

# Entry point script (relative to EXTRACTOR_DIR)
ENTRY_POINT=scripts/run.py

# Results directory on EC2 instance
RESULTS_DIR_INSTANCE=/tmp/extractor-results

# Local results directory
RESULTS_DIR_LOCAL=extractor-results

